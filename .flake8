[flake8]
max-line-length = 125
# E123: Closing brackets indent
# E126: Hanging indents
# E129: Visual indent
# E203: Whitespace before ':' (in accordance with ruff)
# E231: Missing whitespace after ',' (in accordance with ruff)
# E501: Max line length
# E722: bare-except
extend-ignore =
  E123,
  E126,
  E129,
  E203,
  E231,
  E501,
  E722
